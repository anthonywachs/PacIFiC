//----------------------------------------------------------------------
inline
size_t
intVector:: size( void ) const
//----------------------------------------------------------------------
{
   return( LENGTH ) ;
}

//----------------------------------------------------------------------
inline
int const&
intVector:: operator()( size_t i ) const
//----------------------------------------------------------------------
{
   MAC_LABEL( "intVector:: operator()" ) ;
   MAC_CHECK_PRE( i < size() ) ;

   return( VECTOR[i] ) ;
}

//----------------------------------------------------------------------
inline
int&
intVector:: operator()( size_t i )
//----------------------------------------------------------------------
{
   MAC_LABEL( "intVector:: operator()" ) ;
   MAC_CHECK_PRE( i < size() ) ;

   return( VECTOR[i] ) ;
}



//----------------------------------------------------------------------
inline
int const*
intVector:: data( void ) const
//----------------------------------------------------------------------
{   
   return( VECTOR ) ;
}

