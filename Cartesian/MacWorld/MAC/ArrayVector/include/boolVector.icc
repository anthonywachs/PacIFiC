//----------------------------------------------------------------------
inline
size_t
boolVector:: size( void ) const
//----------------------------------------------------------------------
{
   return( LENGTH ) ;
}

//----------------------------------------------------------------------
inline
bool const&
boolVector:: operator()( size_t i ) const
//----------------------------------------------------------------------
{
   MAC_LABEL( "boolVector:: operator()" ) ;
   MAC_CHECK_PRE( i < size() ) ;

   return( VECTOR[i] ) ;
}

//----------------------------------------------------------------------
inline
bool&
boolVector:: operator()( size_t i )
//----------------------------------------------------------------------
{   
   MAC_LABEL( "boolVector:: operator()" ) ;
   MAC_CHECK_PRE( i < size() ) ;

   return( VECTOR[i] ) ;
}



